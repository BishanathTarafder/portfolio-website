      <line x1="-185" y1="-50" x2="-115" y2="25" />
      <line x1="-185" y1="-50" x2="-115" y2="75" />
      
      <line x1="-185" y1="0" x2="-115" y2="-75" />
      <line x1="-185" y1="0" x2="-115" y2="-25" />
      <line x1="-185" y1="0" x2="-115" y2="25" />
      <line x1="-185" y1="0" x2="-115" y2="75" />
      
      <line x1="-185" y1="50" x2="-115" y2="-75" />
      <line x1="-185" y1="50" x2="-115" y2="-25" />
      <line x1="-185" y1="50" x2="-115" y2="25" />
      <line x1="-185" y1="50" x2="-115" y2="75" />
      
      <!-- Layer 2 to 3 -->
      <line x1="-85" y1="-75" x2="-15" y2="-60" />
      <line x1="-85" y1="-75" x2="-15" y2="-20" />
      <line x1="-85" y1="-75" x2="-15" y2="20" />
      <line x1="-85" y1="-75" x2="-15" y2="60" />
      
      <line x1="-85" y1="-25" x2="-15" y2="-60" />
      <line x1="-85" y1="-25" x2="-15" y2="-20" />
      <line x1="-85" y1="-25" x2="-15" y2="20" />
      <line x1="-85" y1="-25" x2="-15" y2="60" />
      
      <line x1="-85" y1="25" x2="-15" y2="-60" />
      <line x1="-85" y1="25" x2="-15" y2="-20" />
      <line x1="-85" y1="25" x2="-15" y2="20" />
      <line x1="-85" y1="25" x2="-15" y2="60" />
      
      <line x1="-85" y1="75" x2="-15" y2="-60" />
      <line x1="-85" y1="75" x2="-15" y2="-20" />
      <line x1="-85" y1="75" x2="-15" y2="20" />
      <line x1="-85" y1="75" x2="-15" y2="60" />
      
      <!-- Layer 3 to 4 -->
      <line x1="15" y1="-60" x2="85" y2="-25" />
      <line x1="15" y1="-60" x2="85" y2="25" />
      
      <line x1="15" y1="-20" x2="85" y2="-25" />
      <line x1="15" y1="-20" x2="85" y2="25" />
      
      <line x1="15" y1="20" x2="85" y2="-25" />
      <line x1="15" y1="20" x2="85" y2="25" />
      
      <line x1="15" y1="60" x2="85" y2="-25" />
      <line x1="15" y1="60" x2="85" y2="25" />
      
      <!-- Layer 4 to 5 -->
      <line x1="115" y1="-25" x2="185" y2="0" />
      <line x1="115" y1="25" x2="185" y2="0" />
    </g>
  </g>
</svg>