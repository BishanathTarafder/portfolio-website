<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <rect width="800" height="600" fill="#1e293b" />
  
  <!-- Background grid -->
  <g stroke="#334155" stroke-width="1" opacity="0.2">
    <line x1="0" y1="50" x2="800" y2="50" />
    <line x1="0" y1="100" x2="800" y2="100" />
    <line x1="0" y1="150" x2="800" y2="150" />
    <line x1="0" y1="200" x2="800" y2="200" />
    <line x1="0" y1="250" x2="800" y2="250" />
    <line x1="0" y1="300" x2="800" y2="300" />
    <line x1="0" y1="350" x2="800" y2="350" />
    <line x1="0" y1="400" x2="800" y2="400" />
    <line x1="0" y1="450" x2="800" y2="450" />
    <line x1="0" y1="500" x2="800" y2="500" />
    <line x1="0" y1="550" x2="800" y2="550" />
    
    <line x1="50" y1="0" x2="50" y2="600" />
    <line x1="100" y1="0" x2="100" y2="600" />
    <line x1="150" y1="0" x2="150" y2="600" />
    <line x1="200" y1="0" x2="200" y2="600" />
    <line x1="250" y1="0" x2="250" y2="600" />
    <line x1="300" y1="0" x2="300" y2="600" />
    <line x1="350" y1="0" x2="350" y2="600" />
    <line x1="400" y1="0" x2="400" y2="600" />
    <line x1="450" y1="0" x2="450" y2="600" />
    <line x1="500" y1="0" x2="500" y2="600" />
    <line x1="550" y1="0" x2="550" y2="600" />
    <line x1="600" y1="0" x2="600" y2="600" />
    <line x1="650" y1="0" x2="650" y2="600" />
    <line x1="700" y1="0" x2="700" y2="600" />
    <line x1="750" y1="0" x2="750" y2="600" />
  </g>
  
  <!-- Main image frame -->
  <rect x="150" y="100" width="500" height="300" rx="5" ry="5" fill="#334155" stroke="#475569" stroke-width="2" />
  
  <!-- Image content - stylized landscape -->
  <g transform="translate(150, 100)">
    <!-- Sky -->
    <rect x="0" y="0" width="500" height="150" fill="#0c4a6e" />
    
    <!-- Mountains -->
    <polygon points="0,150 100,80 150,120 200,60 250,100 300,40 350,90 400,70 450,110 500,90 500,150" fill="#1e293b" />
    
    <!-- Ground -->
    <rect x="0" y="150" width="500" height="150" fill="#0f172a" />
    
    <!-- Trees -->
    <g transform="translate(50, 150)">
      <rect x="-5" y="0" width="10" height="30" fill="#4b5563" />
      <polygon points="-20,-10 0,-40 20,-10" fill="#065f46" />
      <polygon points="-15,0 0,-25 15,0" fill="#065f46" />
    </g>
    
    <g transform="translate(120, 150)">
      <rect x="-6" y="0" width="12" height="40" fill="#4b5563" />
      <polygon points="-25,-15 0,-50 25,-15" fill="#065f46" />
      <polygon points="-20,0 0,-30 20,0" fill="#065f46" />
    </g>
    
    <g transform="translate(400, 150)">
      <rect x="-5" y="0" width="10" height="25" fill="#4b5563" />
      <polygon points="-15,-10 0,-35 15,-10" fill="#065f46" />
      <polygon points="-10,0 0,-20 10,0" fill="#065f46" />
    </g>
    
    <!-- House -->
    <g transform="translate(250, 180)">
      <rect x="-40" y="-30" width="80" height="60" fill="#475569" />
      <polygon points="-45,-30 0,-70 45,-30" fill="#64748b" />
      <rect x="-15" y="10" width="30" height="20" fill="#0c4a6e" />
      <rect x="-30" y="-15" width="15" height="15" fill="#0c4a6e" />
      <rect x="15" y="-15" width="15" height="15" fill="#0c4a6e" />
    </g>
  </g>
  
  <!-- Object detection boxes -->
  <g fill="none" stroke-width="2">
    <rect x="210" y="150" width="100" height="80" stroke="#f472b6" />
    <text x="215" y="145" font-family="Arial, sans-serif" font-size="12" fill="#f472b6">Mountain 98%</text>
    
    <rect x="45" y="230" width="50" height="70" stroke="#38bdf8" />
    <text x="50" y="225" font-family="Arial, sans-serif" font-size="12" fill="#38bdf8">Tree 95%</text>
    
    <rect x="115" y="220" width="60" height="80" stroke="#38bdf8" />
    <text x="120" y="215" font-family="Arial, sans-serif" font-size="12" fill="#38bdf8">Tree 97%</text>
    
    <rect x="395" y="230" width="40" height="60" stroke="#38bdf8" />
    <text x="400" y="225" font-family="Arial, sans-serif" font-size="12" fill="#38bdf8">Tree 92%</text>
    
    <rect x="210" y="250" width="80" height="60" stroke="#a3e635" />
    <text x="215" y="245" font-family="Arial, sans-serif" font-size="12" fill="#a3e635">House 99%</text>
  </g>
  
  <!-- Image processing controls -->
  <g transform="translate(400, 450)">
    <!-- Sliders -->
    <text x="-200" y="-20" font-family="Arial, sans-serif" font-size="14" fill="white">Brightness</text>
    <rect x="-120" y="-30" width="200" height="10" rx="5" ry="5" fill="#334155" />
    <circle cx="-20" cy="-25" r="8" fill="#38bdf8" />
    
    <text x="-200" y="0" font-family="Arial, sans-serif" font-size="14" fill="white">Contrast</text>
    <rect x="-120" y="-10" width="200" height="10" rx="5" ry="5" fill="#334155" />
    <circle cx="0" cy="-5" r="8" fill="#38bdf8" />
    
    <text x="-200" y="20" font-family="Arial, sans-serif" font-size="14" fill="white">Saturation</text>
    <rect x="-120" y="10" width="200" height="10" rx="5" ry="5" fill="#334155" />
    <circle cx="-50" cy="15" r="8" fill="#38bdf8" />
    
    <text x="-200" y="40" font-family="Arial, sans-serif" font-size="14" fill="white">Threshold</text>
    <rect x="-120" y="30" width="200" height="10" rx="5" ry="5" fill="#334155" />
    <circle cx="30" cy="35" r="8" fill="#38bdf8" />
  </g>
  
  <!-- Neural network visualization -->
  <g transform="translate(150, 450)">
    <text x="-100" y="-40" font-family="Arial, sans-serif" font-size="16" fill="white" text-anchor="middle">Computer Vision Model</text>
    
    <!-- Input layer -->
    <rect x="-100" y="-30" width="20" height="20" fill="#f472b6" />
    <rect x="-100" y="-5" width="20" height="20" fill="#f472b6" />
    <rect x="-100" y="20" width="20" height="20" fill="#f472b6" />
    
    <!-- Hidden layers -->
    <rect x="-50" y="-30" width="20" height="20" fill="#38bdf8" />
    <rect x="-50" y="-5" width="20" height="20" fill="#38bdf8" />
    <rect x="-50" y="20" width="20" height="20" fill="#38bdf8" />
    
    <rect x="0" y="-30" width="20" height="20" fill="#a3e635" />
    <rect x="0" y="-5" width="20" height="20" fill="#a3e635" />
    <rect x="0" y="20" width="20" height="20" fill="#a3e635" />
    
    <!-- Output layer -->
    <rect x="50" y="-17.5" width="20" height="20" fill="#fbbf24" />
    <rect x="50" y="7.5" width="20" height="20" fill="#fbbf24" />
    
    <!-- Connections -->
    <g stroke="#475569" stroke-width="1">
      <!-- Layer 1 to 2 -->
      <line x1="-80" y1="-20" x2="-50" y2="-20" />
      <line x1="-80" y1="-20" x2="-50" y2="5" />
      <line x1="-80" y1="-20" x2="-50" y2="30" />
      
      <line x1="-80" y1="5" x2="-50" y2="-20" />
      <line x1="-80" y1="5" x2="-50" y2="5" />
      <line x1="-80" y1="5" x2="-50" y2="30" />
      
      <line x1="-80" y1="30" x2="-50" y2="-20" />
      <line x1="-80" y1="30" x2="-50" y2="5" />
      <line x1="-80" y1="30" x2="-50" y2="30" />
      
      <!-- Layer 2 to 3 -->
      <line x1="-30" y1="-20" x2="0" y2="-20" />
      <line x1="-30" y1="-20" x2="0" y2="5" />
      <line x1="-30" y1="-20" x2="0" y2="30" />
      
      <line x1="-30" y1="5" x2="0" y2="-20" />
      <line x1="-30" y1="5" x2="0" y2="5" />
      <line x1="-30" y1="5" x2="0" y2="30" />
      
      <line x1="-30" y1="30" x2="0" y2="-20" />
      <line x1="-30" y1="30" x2="0" y2="5" />
      <line x1="-30" y1="30" x2="0" y2="30" />
      
      <!-- Layer 3 to 4 -->
      <line x1="20" y1="-20" x2="50" y2="-7.5" />
      <line x1="20" y1="-20" x2="50" y2="17.5" />
      
      <line x1="20" y1="5" x2="50" y2="-7.5" />
      <line x1="20" y1="5" x2="50" y2="17.5" />
      
      <line x1="20" y1="30" x2="50" y2="-7.5" />
      <line x1="20" y1="30" x2="50" y2="17.5" />
    </g>
  </g>
  
  <!-- Title -->
  <text x="400" y="50" font-family="Arial, sans-serif" font-size="24" fill="white" text-anchor="middle">Computer Vision System</text>
</svg>